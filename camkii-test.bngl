begin model
begin parameters
	NA 6.02e23	# molecues/mol
	f  1		# fraction of the cell to simulate
	Vo f*1e-10	# extracellular volume=1/cell_density (L)
	V  f*3e-12	# cytoplasmic volume (L)
	
	#  initial amounts
	CaMCa4_init	
	bCaMKII_init

	#  Rate constants
	kf1	# Cuu -> Cb
	kr1
	kf2	# Cb -> Cbp
	kr2	
	kf3	# Cbp -> Cpu
	kr3	
	kf4	# Cpu -> Cuu
	kr4	
	
end parameters

begin molecule types
	CaMCa4 (CmK)
	bCaMKII(x~Cuu~Cb~Cbp~Cpu, x~Cuu~Cb~Cbp~Cpu, x~Cuu~Cb~Cbp~Cpu, x~Cuu~Cb~Cbp~Cpu)
	
end molecule types

begin seed species
	CaMCa4(CmK)	CaMCa4_init
	bCaMKII(x~Cuu, x~Cuu, x~Cuu, x~Cuu)	bCaMKII_init
end seed species

begin observables
	01: Molecules bCaMKII_autoinhibit	bCaMKII(x~Cuu, x~Cuu, x~Cuu, x~Cuu)
	02: Molecules bCaMKII_bound		bCaMKII(x~Cb)
end observables

begin reaction rules
	R1: bCaMKII(x~Cuu) + CaMCa4(CmK) <-> bCaMKII(x~Cb)  kf1,kr1
	R2: bCaMKII(x~Cb, x~Cb~Cbp~Cpu) <-> bCaMKII(x~Cbp) kf2,kr2
	R3: bCaMKII(x~Cb~Cbp~Cpu, x~Cb) <-> bCaMKII(x~Cbp) kf2,kr2
	R4: bCaMKII(x~Cbp) <-> bCaMKII(x~Cpu) kf3,kr3
	R5: bCaMKII(x~Cpu) <-> bCaMKII(x~Cuu) kf4,kr4
end reaction rules
end model
